  {
    "layer": "top",
    "position": "top",
    "reload_style_on_change": true,
    "margin-top": 6,
    "margin-right": 15,
    "margin-left": 15,
    "spacing": 4,
    "modules-left": [
      "hyprland/workspaces",
      "hyprland/window"
    ],
    "modules-center": [
      //"group/expand-music-control",
      "custom/mediaplayer",
      "custom/kb_layout"
    ],
    "modules-right": [
      "custom/notification",
      "group/expand",
      "pulseaudio",
      "network",
      "battery",
      "clock"
    ],
    "backlight/slider": {
      "min": 0,
      "max": 100,
      "orientation": "horizontal",
      "device": "amdgpu_bl1"
    },
    "group/backlight_slider": {
      "orientation": "horizontal",
      "drawer": {
        "transition-duration": 600,
        "transition-to-left": true,
        "click-to-reveal": true
      },
      "modules": [
        "backlight",
        "backlight/slider"
      ]
    },
    "group/expand": {
      "orientation": "horizontal",
      "drawer": {
        "transition-duration": 600,
        "transition-to-left": true,
        "click-to-reveal": false
      },
      "modules": [
        "custom/expand",
        "custom/hyprshade",
        "idle_inhibitor",
        "tray",
        "custom/pacman",
        "power-profiles-daemon",
        "group/backlight_slider",
        "bluetooth",
        "custom/endpoint"
      ]
    },
    "custom/kb_layout": {
      "exec": "~/.config/waybar/scripts/kb_layout.sh",
      "format": "{}",
      "interval": 1,
      "return-type": "json",
      "on-click": "~/.config/hypr/scripts/toggle_kb_layout.sh"
    },
    "hyprland/workspaces": {
      "disable-scroll": true,
      "all-outputs": false,
      "warp-on-scroll": false,
      "format": "{name}",
      "persistent-workspaces": {
        "*": [
          1,
          2,
          3
        ]
      }
    },
    "hyprland/window": {
      "format": "ï” {title}",
      "max-length": 30
    },
    "power-profiles-daemon": {
      "format": "{icon}",
